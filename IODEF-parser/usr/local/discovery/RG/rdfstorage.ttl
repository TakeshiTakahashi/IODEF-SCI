@prefix rdfs:	  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ja:       <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix sdb:      <http://jena.hpl.hp.com/2007/sdb#> .

# For use with Fuseki
[] ja:loadClass "com.hp.hpl.jena.sdb.SDB" .
sdb:DatasetStore  rdfs:subClassOf  ja:RDFDataset .

<#dataset> rdf:type sdb:DatasetStore ;
    sdb:store <#store> .

<#store> rdf:type sdb:Store ;
    sdb:layout         "layout2/index" ;
#    sdb:layout         "layout2/hash" ;
    sdb:connection     <#conn> .

<#conn> rdf:type sdb:SDBConnection ;
     sdb:jdbcURL       "jdbc:mysql://localhost/iodef?useUnicode=true&characterEncoding=UTF-8" ;
     sdb:sdbType       "MySQL" ;
     sdb:sdbHost       "localhost" ;
     sdb:sdbName       "SDB" ;
     sdb:sdbUser        "root" ;
     sdb:sdbPassword    "discovery" ;
    .
